/**index.wxss**/
@import "../../wxParse/wxParse.wxss"
page{
	background: #f2f2f2;
}
.page-body image{
	width:100%;
	height:370rpx;
}
swiper{
	height:370rpx;
}
.swiper-block{
  height: 370rpx;
  width: 100%;
}
.swiper-item{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  overflow:unset;
}

.mask{
	width:100%;
	height:100%;
	background:rgba(0,0,0,0.5);
	position:fixed;
	top:0;
	left:0;
	z-index:999;
	overflow:hidden;
}
.discount{
	width:100%;
	height:670rpx;
	position:absolute;
	top:30%;
	z-index:999;
}
.discount_cont{
	width:750rpx;
	height:642rpx;
	position: relative;
}
.discount_box{
	width:100%;
	height:642rpx;
	position: absolute;
	left:0;
	top:0;
	margin:0 auto;
}
.discount_cont image{
	width:100%;
	height:100%;
}
.discount_in{
	width:530rpx;
	height:auto;
	margin:0 auto;
}
.discount_tlt{
	margin-top: 150rpx;
	text-align: center;
}
.discount_bg{
	width:390rpx;
	height:148rpx;
	margin:20rpx auto;
	position: relative;

}
.discount_number{
	width:100%;
	height:100%;
	position: absolute;
	left:0;
	top:0;
}
.discount_bg image{
	width:390rpx;
	height:148rpx;
	margin:0 auto;
}
.discount_left{
	width:59%;

}
.discount_right{
	width:41%;
}
.discount_line{
	width:20%;
	height:1px;
	background:#fff;
}
.discount_center{
	width:50%;
	text-align:center;
}
.discount_btn{
	width:300rpx;
	height:90rpx;
	margin:20rpx auto;
	position: relative;
}
.discount_btn image{
	width:100%;
	height:100%;
}
.discount_btn form{
	width:100%;
	height:100%;
	position: absolute;
	left:0;
	top:0;
}
.discount_btn button{
	font-size:44rpx;
	text-align: center;
	line-height:90rpx;
	width:300rpx;
	height:90rpx;
	margin:0 auto;
	background: none;
}
.close{
	width:56rpx;
	height:56rpx;
	margin:0 auto;
}
.close image{
	width:56rpx;
	height:56rpx;
}
.logo{
	height:110rpx;
	width:100%;
	border-bottom: solid 1px #e0e0e0;
}
.logo_img image{
	width:60rpx;
	height:60rpx;
	margin-right: 20rpx;
}
.store{
	width:100%;
	height:124rpx;
}
.store_cont{
	width:100%;
	height:56rpx;
}
.store_in{
	width:100%;
	height:100%;
}
.address{
	width:10%;
}
.address image{
	width:48rpx;
	height:36rpx;
	margin:0 auto;
}
.address_detail{
	width:73%;
}
.phone{
	width:13.5%;
}
.phone image{
	width:52rpx;
	height:52rpx;
	margin:0 auto;
}
.volume{
	height:96rpx;
}
.mall_tlt_txt{
	color:#333;
	width:100%;
	height:28rpx;
	text-align:center;
	position:absolute;
	left:0;
	top:0;
}
.mall_tlt{
	position:relative;
	text-align:center;
	width:264rpx;
	overflow:hidden;
	margin:0 auto;
	height:30rpx;
	line-height:30rpx;
}
.mall_tlt image{
	width:264rpx;
	height:28rpx;
}
.mall_tlt span{
	width:8rpx;
	height:8rpx;
	border-radius: 50%;
	margin:0 8rpx;
	display: inline-block;
}
.span1{
	background:#ff6b73;
}
.span2{
	background:#ff9ec8;
}
.span3{
	background:#ff6b73;
}
.recommend_left{
	width:48.5%;
	border-radius: 10rpx;
	padding-bottom: 20rpx;
}
.recommend_left image{
	width:100%;
	height: 400rpx;
}
.product_name{
	height:80rpx;
	line-height: 80rpx;
}
.index_product{
	flex-wrap: wrap;
}
.volume_item,.volume_buy{
	width:27%;
}
.volume_buy{
	text-align:right;
}
.volume_detail{
	width:46%;
}
.volume_cont{
	width:100%;
	height:148rpx;
}
.item_in{
	width:166rpx;
	height:110rpx;
	background: linear-gradient(to right,rgba(249,197,55,1.00),rgba(247,137,46,1.00));
	border-radius: 20rpx;
}
.item_top{
	width:100%;
	height:60rpx;
	line-height:60rpx;
	border-bottom:dashed 1px #fff;
	text-align: center;
}
.item_bottom{
	line-height:50rpx;
}
.buy_btn{
	width:150rpx;
	height:58rpx;
	border-radius: 30rpx;
	border:solid 1px #666;
	text-align:center;
	line-height: 58rpx;
	text-align: center;
	margin-left:50rpx;
	margin-bottom: 10rpx;
}

.big_title{
	width:100%;
	height:102rpx;
	margin:0 auto;
}
.tlt_left{
	width:36%;
	height:36rpx;
}
.tlt_left image{
	width:100%;
	height:2rpx;
}
.tlt_center{
	width:28%;
	text-align:center;
	font-weight:700;
	height:36rpx;
	border:solid 1px #e0e0e0;
	border-radius: 30rpx;
	text-align: center;
	line-height:36rpx;
}
.merchant image{
	width:36rpx;
	height:32rpx;
	margin-right: 20rpx;
}
.condition{
	width:108rpx;
	height:32rpx;
	border-radius: 10rpx;
	text-align: center;
	line-height:32rpx;
	border:solid 1px #c4010c;
	margin-right: 20rpx;
}
.store_img image{
	width:100%;
	height:400rpx;
	border-radius: 20rpx;
}
.store_img{
	margin-bottom: 30rpx;
}
.merchant{
	margin-bottom:30rpx;
}
.detail_img image{
	width:100%;
	height:380rpx;
}